DROP TABLE IF EXISTS qop.osm_pois;

CREATE TABLE qop.osm_pois (
	gid serial4 NOT NULL,
	nodeid serial8,
	"mainkey" text,
	"mainval" text,
	"name" text NULL,
	"tags" jsonb NULL,
	geom geometry(point, 4326),
	CONSTRAINT aerzte_pkey PRIMARY KEY (gid)
);
CREATE INDEX osm_pois_geom_gist ON qop.osm_pois USING gist (geom);